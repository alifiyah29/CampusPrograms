#include<iostream>
using namespace std;

int arrMax(int a[], int n){
   int max=a[0];
   for(int i=0;i<n;i++){
       if(max<=a[i])
          max=a[i];
   }
   cout<<"Max = "<<max;
   return max;
}

int main(){
    int arr[100],size;
    cin>>size;
    for(int i=0;i<size;i++)
        cin>>arr[i];
    arrMax(arr,size);    
    return 0;
}
