#include <iostream>
using namespace std;

int main()
{
    int n1,n2;
    cin>>n1>>n2;
    int a[n1][n2];
    for(int i=0;i<n1;i++){
        for(int j=0;j<n2;j++)
            cin>>a[i][j];
    }
    int min=a[0][0],max=a[0][0];
    for(int i=0;i<n1;i++){
        for(int j=0;j<n2;j++){
            if(a[i][j]<min)
                min=a[i][j];
            if(a[i][j]>max)
                max=a[i][j];                
        }
    }
    cout<<"Min = "<<min<<" Max = "<<max;
    return 0;
}
