#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int num,rem,sum=0,crem;
    cin>>num;
    int n=num;
    while(n!=0){
        rem = n%10;
        crem = pow(rem,3);
        sum+=crem; 
        n/=10;
    }
    if(sum==num)
        cout<<"Armstrong number";
    else
        cout<<"Not Armstrong Number";
    return 0;
}
